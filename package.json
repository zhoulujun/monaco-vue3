  "license": "MIT",
  "bugs": {
    "url": "https://github.com/zhoulujun/monaco-vue3/issues"
  },
  "homepage": "https://github.com/zhoulujun/monaco-vue3#readme",
  "dependencies": {
    "bkui-vue": "^0.0.2-beta.25",
    "monaco-editor": "^0.41.0",
    "monaco-promql": "^1.7.4"
  },
  "peerDependencies": {
    "bkui-vue": "^0.0.2-beta.25",
    "monaco-promql": "^1.7.4",
    "vue": "^3.2.37"
  },
  "devDependencies": {
    "@blueking/eslint-config-bk": "^2.1.0-beta.9",
